            "match": "(').*$\\n?",
            "name": "comment.line.apostrophe.brightscript"
        },
        {
            "captures":{
                "1":{
                    "name": "punctuation.definition.comment.brightscript"
                }
            },
            "match": "^\\s*?(?i:rem\\s).*$\\n?",
            "name": "comment.line.rem.brightscript"
        },
