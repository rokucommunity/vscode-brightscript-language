            "include": "#literal"
        },
        {
            "include": "#program_statements"
        },
        {
            "include": "#operators"
        },
        {
            "include": "#support_functions"
        },
        {
            "include": "#storage_types"
        },
        {
            "include": "#constant_numeric"
        },
        {
            "captures": {
                "1": {
                    "name": "punctuation.definition.comment.brightscript"
